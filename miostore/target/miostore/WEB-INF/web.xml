<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
  <welcome-file-list>
    <welcome-file>home</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>HomController</servlet-name>
    <servlet-class>controller.HomController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>controller.LoginController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>controller.LogoutController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>OrderController</servlet-name>
    <servlet-class>controller.OrderController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddCartController</servlet-name>
    <servlet-class>controller.AddCartController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ViewCartController</servlet-name>
    <servlet-class>controller.ViewCartController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditCart</servlet-name>
    <servlet-class>controller.EditCartController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteCartController</servlet-name>
    <servlet-class>controller.DeleteCartController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CheckOutServlet</servlet-name>
    <servlet-class>controller.CheckOutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginFbController</servlet-name>
    <servlet-class>controller.LoginFbController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SelectAddressController</servlet-name>
    <servlet-class>controller.SelectAddressController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeliveryMethodServlet</servlet-name>
    <servlet-class>controller.DeliveryMethodServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ViewTypeController</servlet-name>
    <servlet-class>controller.ViewTypeController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchController</servlet-name>
    <servlet-class>controller.SearchController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ajaxServlet</servlet-name>
    <servlet-class>controller.ajaxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ajaxServlet</servlet-name>
    <url-pattern>/vnpayajax</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>HomController</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OrderController</servlet-name>
    <url-pattern>/order</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddCartController</servlet-name>
    <url-pattern>/addcart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewCartController</servlet-name>
    <url-pattern>/viewcart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditCart</servlet-name>
    <url-pattern>/editcart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteCartController</servlet-name>
    <url-pattern>/deletecart</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckOutServlet</servlet-name>
    <url-pattern>/checkout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginFbController</servlet-name>
    <url-pattern>/loginfb</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SelectAddressController</servlet-name>
    <url-pattern>/selectaddress</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeliveryMethodServlet</servlet-name>
    <url-pattern>/deliverymethod</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewTypeController</servlet-name>
    <url-pattern>/type</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchController</servlet-name>
    <url-pattern>/searchitem</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>
      30
    </session-timeout>
  </session-config>
</web-app>